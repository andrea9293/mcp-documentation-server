{
  "name": "mcp-documentation-server-ts",
  "version": "1.0.0",
  "description": "A TypeScript MCP server for document management and semantic search",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-documentation-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx src/server.ts",
    "dev": "fastmcp dev src/server.ts",
    "dev:direct": "npx tsx src/server.ts",
    "dev:build": "npm run build && node dist/index.js",
    "inspect": "npx fastmcp inspect src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "documentation",
    "search",
    "embeddings"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@tsconfig/node22": "^22.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@types/node": "^24.0.1",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^16.5.0",
    "fastmcp": "^1.27.3",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "zod": "^3.25.64"
  }
}
